{
  "name": "@nnest/winston",
  "version": "0.0.1",
  "private": true,
  "license": "MIT",
  "engines": {
    "npm": "please-use-yarn"
  },
  "publishConfig": {
    "directory": "dist"
  },
  "scripts": {
    "prebuild": "npm run clean",
    "clean": "rimraf dist",
    "build": "tsc -p tsconfig.build.json",
    "watch": "tsc -p tsconfig.build.json --watch --preserveWatchOutput",
    "format": "prettier --write \"{src,test}/**/*.ts\"",
    "lint": "eslint \"{src,test}/**/*.ts\"",
    "lint:fix": "eslint \"{src,test}/**/*.ts\" --fix"
  },
  "dependencies": {
    "@nestjs/common": "^7.1.3",
    "@nestjs/config": "^0.5.0",
    "nest-winston": "^1.3.6",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "rimraf": "^3.0.2",
    "typescript": "^3.9.5"
  }
}
