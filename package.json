{
  "name": "vocaburst",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "build:watch": "nest build -w",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js -f src/typeorm/bootstrap.ts",
    "migration:generate": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js -f src/typeorm/bootstrap.ts migration:generate -n",
    "migration:show": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js -f src/typeorm/bootstrap.ts migration:show",
    "migration:run": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js -f src/typeorm/bootstrap.ts migration:run",
    "migration:revert": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js -f src/typeorm/bootstrap.ts migration:revert",
    "schema:log": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js -f src/typeorm/bootstrap.ts schema:log",
    "schema:sync": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js -f src/typeorm/bootstrap.ts schema:sync"
  },
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "@nestjs/common": "^7.0.0",
    "@nestjs/config": "^0.5.0",
    "@nestjs/core": "^7.0.0",
    "@nestjs/graphql": "^7.4.0",
    "@nestjs/jwt": "^7.0.0",
    "@nestjs/passport": "^7.0.0",
    "@nestjs/platform-express": "^7.0.0",
    "@nestjs/swagger": "^4.5.9",
    "@nestjs/typeorm": "^7.1.0",
    "apollo-server-express": "^2.14.1",
    "bcrypt": "^4.0.1",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.12.2",
    "graphql": "^15.0.0",
    "graphql-tools": "^6.0.5",
    "lodash": "^4.17.15",
    "module-alias": "^2.2.2",
    "nest-winston": "^1.3.4",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.2.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.5.4",
    "sqlite3": "^4.2.0",
    "swagger-ui-express": "^4.1.4",
    "typeorm": "^0.2.25",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.0.0",
    "@nestjs/schematics": "^7.0.0",
    "@nestjs/testing": "^7.0.0",
    "@types/bcrypt": "^3.0.0",
    "@types/express": "^4.17.3",
    "@types/hapi__joi": "^17.1.0",
    "@types/jest": "25.1.4",
    "@types/lodash": "^4.14.154",
    "@types/node": "^13.9.1",
    "@types/passport-jwt": "^3.0.3",
    "@types/passport-local": "^1.0.33",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "^2.23.0",
    "@typescript-eslint/parser": "^2.23.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "jest": "^25.1.0",
    "prettier": "^1.19.1",
    "supertest": "^4.0.2",
    "ts-jest": "25.2.1",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.2",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "_moduleAliases": {
    "@root": "./src",
    "@nnest/api": "src/api",
    "@nnest/auth": "src/auth",
    "@nnest/typeorm": "src/typeorm",
    "@nnest/users": "src/users",
    "@nnest/winston": "src/winston"
  }
}
